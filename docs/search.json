---
layout: null
---
{% assign pages = site.pages | where_exp: "page", "page.title" %}
{% assign items = "" | split: "" %}
{% for page in pages %}
  {% unless page.url contains '/archive/' or page.url contains '/assets/' or page.url contains '/presentations/' or page.url == '/search.json' %}
    {% capture item %}
    {
      "title": {{ page.title | jsonify }},
      "url": {{ page.url | relative_url | jsonify }},
      "content": {{ page.content | markdownify | strip_html | strip_newlines | jsonify }}
    }
    {% endcapture %}
    {% assign items = items | push: item %}
  {% endunless %}
{% endfor %}
[{{ items | join: "," }}]
